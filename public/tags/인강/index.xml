 text-align: center;
 gap: 16px;
 }

 .cta-icon {
 width: 56px;
 height: 56px;
 margin: 0 auto;
 }

 .cta-message {
 font-size: 14px;
 }

 .cta-button {
 width: 100%;
 padding: 14px 24px;
 font-size: 14px;
 }
}

 
.cta-button:focus {
 outline: 3px solid #667eea;
 outline-offset: 4px;
}

 
@media print {
 .cta-consultation {
 display: none !important;
 }
}
