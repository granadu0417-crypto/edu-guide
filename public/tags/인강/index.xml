 box-shadow: 0 8px 24px rgba(0, 0, 0, 0.06);
 border-color: #ced4da;
}

.cta-content {
 display: flex;
 align-items: center;
 gap: 16px;
}

.cta-icon {
 flex-shrink: 0;
 width: 48px;
 height: 48px;
 display: flex;
 align-items: center;
 justify-content: center;
 background: linear-gradient(135deg, #FEE500 0%, #FFD700 100%);
 border-radius: 12px;
 color: #3C1E1E;
}

.cta-text {
 flex: 1;
}

.cta-message {
 margin: 0;
 font-size: 15px;
 color: #495057;
 line-height: 1.6;
 font-weight: 500;
}

.cta-button {
 flex-shrink: 0;
 padding: 12px 24px;
 text-decoration: none;
